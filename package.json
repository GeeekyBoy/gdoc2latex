{
  "name": "gdoc2latex",
  "version": "1.0.0",
  "description": "Converts Google Docs files to Latex",
  "main": "./lib/index.js",
  "bin": {
    "gdoc2latex": "./lib/index.js"
  },
  "scripts": {
    "start:watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start": "ts-node src/index.ts",
    "build": "tsc -p ./tsconfig.json"
  },
  "author": "Adam Jones (domdomegg)",
  "dependencies": {
    "commander": "^6.1.0",
    "he": "^1.2.0",
    "himalaya": "^1.1.0"
  },
  "devDependencies": {
    "@types/he": "^1.1.1",
    "@types/node": "^14.11.10",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
