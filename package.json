  "devDependencies": {
    "@types/he": "^1.1.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
